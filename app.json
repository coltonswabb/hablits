{
  "expo": {
    "name": "Hablits",
    "slug": "hablits",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "description": "Build better habits with your virtual pet companion. Track daily habits, create routines, and watch your pet grow as you stay consistent.",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hablits.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This app does not track you.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "package": "com.hablits.app",
      "versionCode": 1,
      "permissions": [
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "23115a03-2663-4d4b-a53f-86cdb5907761"
      }
    },
    "owner": "hablits",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/23115a03-2663-4d4b-a53f-86cdb5907761"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#667eea"
        }
      ]
    ]
  }
}
